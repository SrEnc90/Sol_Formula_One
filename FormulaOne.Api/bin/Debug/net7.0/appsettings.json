{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Hangfire": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db;",
    "HangfireConnection": "hangfire.db"
  },
  "DatabaseConfig": {
    "TimeoutTime": 30,
    "DetailError": true,
    "SensitiveDataLogging": true,
    
    
    "MaxRetryCount": 3,
    "MaxRetryDelay": 5
  }
}
